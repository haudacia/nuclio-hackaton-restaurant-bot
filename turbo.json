{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".dist/**"
      ]
    },
    "dev": {
      "dependsOn": [
        "^dev"
      ],
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": [
        "^test"
      ],
      "cache": false,
      "persistent": false
    },
    "lint": {}
  }
}